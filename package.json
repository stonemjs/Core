{
  "name": "@stone-js/core",
  "version": "0.0.1",
  "description": "StoneJS core foundation",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.mts",
  "typings": "./dist/index.d.mts",
  "repository": "git@github.com:noowow-community/core.git",
  "author": "Mr Stone <pierre.evens16@gmail.com>",
  "license": "MIT",
  "files": [
    "/dist"
  ],
  "keywords": [
    "Core",
    "StoneJS"
  ],
  "homepage": "https://github.com/noowow-community/core#readme",
  "bugs": {
    "url": "https://github.com/noowow-community/core/issues"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "types": "tsc",
    "lint": "eslint src --ext mjs",
    "lint:test": "eslint tests --ext mjs",
    "lint:fix": "eslint --fix --ext mjs src/ tests/",
    "release": "yarn build && npm publish",
    "build": "yarn lint && webpack --env prod && yarn types",
    "build:dev": "yarn lint && webpack --env dev && yarn types",
    "test": "yarn lint:test && mocha --experimental-modules --no-warnings test/**/*.spec.mjs"
  },
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "babel-loader": "^9.1.3",
    "chai": "^4.3.8",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
